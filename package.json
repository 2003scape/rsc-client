{
  "name": "@2003scape/rsc-client",
  "version": "1.0.1",
  "description": "runescape classic web client",
  "main": "./src/mudclient.js",
  "scripts": {
    "build-asm": "asc --initialMemory 750 --exportRuntime -O3 --noAssert src/asm/index.ts -o dist/index.bundle.wasm",
    "build-dev": "browserify -t brfs -t bulkify -p esmify index.js > dist/index.bundle.js",
    "build": "browserify -t brfs -t bulkify -p esmify -p tinyify index.js > dist/index.bundle.min.js",
    "start": "st -p 1337 -nc -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2003scape/rsc-client.git"
  },
  "keywords": [
    "runescape",
    "classic",
    "rsc",
    "client",
    "mmorpg",
    "game"
  ],
  "author": "2003Scape Team",
  "license": "AGPL-3.0+",
  "bugs": {
    "url": "https://github.com/2003scape/rsc-client/issues"
  },
  "homepage": "https://github.com/2003scape/rsc-client#readme",
  "devDependencies": {
    "@assemblyscript/loader": "^0.19.1",
    "@ledgerhq/compressjs": "^1.3.2",
    "alawmulaw": "^6.0.0",
    "assemblyscript": "^0.19.1",
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "bulk-require": "^1.0.1",
    "bulkify": "1.4.0",
    "eslint": "^7.26.0",
    "esmify": "^2.1.1",
    "long": "^4.0.0",
    "ndarray": "^1.0.19",
    "pcm-player": "0.0.11",
    "sleep-promise": "^9.1.0",
    "st": "^2.0.0",
    "tga-js": "^1.1.1",
    "tinyify": "^3.0.0"
  },
  "dependencies": {
    "brfs": "^2.0.2"
  }
}
